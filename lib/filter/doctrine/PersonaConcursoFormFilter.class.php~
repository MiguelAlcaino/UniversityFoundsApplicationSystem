 * @version    SVN: $Id: sfDoctrineFormFilterTemplate.php 23810 2009-11-12 11:07:44Z Kris.Wallsmith $
 */
class PersonaConcursoFormFilter extends BasePersonaConcursoFormFilter
{
  public function configure()
  {
   $this->widgetSchema['convocatoria_id'] = new sfWidgetFormDoctrineChoice(array(
  'model'     => 'Convocatoria',
  'add_empty' => false,
));
  }
  
  
}
